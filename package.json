{
	"name": "@gilbarco/vpos-dashboard",
	"packageName": "gilbarco-vpos-dashboard",
	"version": "0.0.1",
	"private": true,
	"main": "./dist/vpos-dashboard.js",
	"engines": {
		"node": ">=18.12.1"
	},
	"author": "Ben Henning",
	"license": "ISC",
	"homepage": "https://github.com/bhenningdearx-glitch/vpos-dashboard/blob/main/README.md",
	"publishConfig": {
		"access": "restricted",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"type": "git",
		"url": "git@ssh.dev.azure.com:v3/fiscalyze/VPOS/vpos-dashboard"
	},
	"scripts": {
		"test": "jest",
		"test:ci": "jest --forceExit",
		"test:fix": "jest --detectOpenHandles",
		"test:watch": "jest --watch",
		"test:changed": "jest --onlyChanged",
		"test:coverage": "jest --coverage",
		"start": "node dist/vpos-dashboard.js start",
		"dev": "cross-env NODE_OPTIONS='--expose-gc' ts-node-dev --respawn --transpile-only -- src/vpos-dashboard.ts",
		"dev:debug": "cross-env NODE_OPTIONS='--inspect-brk --expose-gc' ts-node-dev --respawn --transpile-only src/vpos-dashboard.ts",
		"prod": "npm run build && cross-env NODE_ENV=production node ./dist/vpos-dashboard.js start",
		"format": "prettier --check ./{src,tests}/**/*.{ts,json}",
		"format:fix": "prettier --write ./{src,tests}/**/*.{ts,json}",
		"build": "npm run format:fix && tsc",
		"build:prod:clean": "cd .builder/build && npm prune --production",
		"git:push": "git push && git push origin --tags",
		"publish:patch": "npm version patch && git push && git push origin --tags",
		"publish:minor": "npm version minor && git push && git push origin --tags"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.2",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.15",
		"@types/node": "^24.10.1",
		"@types/ws": "^8.5.14",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.22.0",
		"cpx": "^1.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.19.0",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.9.3"
	}
}